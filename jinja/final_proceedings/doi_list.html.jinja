+++
title = """[{{contributions|length}}]"""
+++

<section>
  <ul>
    {% for contribution in contributions %}
    {% if contribution.code and contribution.is_qa_approved %}
    <li>
      <a onclick="openUrl(`doi/{{ contribution.code|lower }}/index.html`)" href="javascript:void(0);">
        <b>{{ contribution.code }}</b>
        ({{ contribution.title }})
      </a>
    </li>
    {% endif %}
    {% endfor %}
  </ul>
</section>