+++
title = """[{{institutes|length}}]"""
+++

<section>
  <ul>
    {% for institute in institutes %}
    {% if institute.id: %}
    <li>
      <a data-href="institute/{{ institute.id|lower }}/index.html" 
        onclick="openUrl(`institute/{{ institute.id|lower }}/index.html`)" 
        href="javascript:void(0);">{{ institute.name }}</a>
    </li>
    {% endif %}
    {% endfor %}
  </ul>
</section>