---
title: "[{{contributions|length}}]"
date: 2023-01-02T12:06:30+01:00
draft: false
---

<h1>
    {{ session.code }} -
    {{ session.title|e }} -
    {{ session.start.strftime('%d %B %Y, %H:%M') }}
    -
    {{ session.end.strftime('%H:%M') }}
</h1>

<section>
    {% for convener in session.conveners %}
    <h4>Chair: {{ convener.first|e }} {{ convener.last|e }} ({{ convener.affiliation|e }})</h4>
    {% endfor %}
</section>

{% import 'contribution_detail.html.jinja' as contribution_detail %}

{% for contribution in contributions %}
{{ contribution_detail.render(contribution, loop.cycle('odd', 'even')) }}
{% endfor %}