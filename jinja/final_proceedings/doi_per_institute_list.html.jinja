+++
title = "[{{institutes|length}}]"
+++

<section>
  <ul>
    {% for institute in institutes %}
    {% if institute.id: %}
    <li>
      <a onclick="openUrl(`doi_per_institute/{{ institute.id|lower }}/index.html`)" href="javascript:void(0);">
        {{ institute.name }}
      </a>
    </li>
    {% endif %}
    {% endfor %}
  </ul>
</section>