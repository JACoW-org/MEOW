<h1>FEL2022 - DOI per Institute</h1>

<section>
  <ul>
    {% for institute in institutes %}
    {% if institute.id: %}
    <li class="{{ loop.cycle('odd', 'even') }}">
      <a href="./{{ institute.id|lower }}">
        {{ institute.name }}
      </a>
    </li>
    {% endif %}
    {% endfor %}
  </ul>
</section>